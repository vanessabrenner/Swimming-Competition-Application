<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="swimmingcompetition.client.gui.LoginController">
   <children>
      <Label fx:id="login_lbl" layoutX="264.0" layoutY="83.0" text="Login">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <GridPane layoutX="205.0" layoutY="154.0" prefHeight="135.0" prefWidth="190.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="190.0" minWidth="10.0" prefWidth="190.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="username_txtf" promptText="Username" />
            <PasswordField fx:id="password_pswdf" promptText="Password" GridPane.rowIndex="1" />
            <Button fx:id="login_btn" mnemonicParsing="false" onAction="#showProfile" text="Login" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
            <Label fx:id="error_lbl" prefHeight="18.0" prefWidth="132.0" textFill="RED" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
         </children>
      </GridPane>
      <Button layoutX="568.0" layoutY="378.0" mnemonicParsing="false" onAction="#closeButtonClicked" text="Exit">
         <font>
            <Font size="9.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
